    SUBROUTINE FT.ADVICE.NO.TEST

    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT GLOBUS.BP I_F.FUNDS.TRANSFER
    $INSERT JBL.BP I_F.ABL.H.NM.ER
    $INSERT GLOBUS.BP I_F.TELLER

    GOSUB INIT
    GOSUB OPENFILES
    GOSUB PROCESS
    RETURN

****
INIT:
****
    FN.FT = 'FBNK.FUNDS.TRANSFER'
    F.FT = ''
    FN.FT.HIS = 'FBNK.FUNDS.TRANSFER$HIS'
    F.FT.HIS = ''
    FN.TT = 'FBNK.TELLER'
    F.TT = ''
    FN.TT.HIS = 'FBNK.TELLER$HIS'
    F.TT.HIS = ''

    CALL GET.LOC.REF('FUNDS.TRANSFER','ADVISE.REF.NO',Y.ADVISE.REF.NO.POS)
    CALL GET.LOC.REF('TELLER','ADVISE.REF.NO',Y.TT.ADVISE.REF.NO.POS)
    RETURN
**********
OPENFILES:
**********
    CALL OPF(FN.FT,F.FT)
    CALL OPF(FN.FT.HIS,F.FT.HIS)
    CALL OPF(FN.TT,F.TT)
    CALL OPF(FN.TT.HIS,F.TT.HIS)
    RETURN

********
PROCESS:
********
!    DEBUG
    Y.ID  = ID.NEW
    IF Y.ID[1,2] EQ "FT" THEN
        CALL F.READ(FN.FT,Y.FT.ID,R.FT,F.FT,FT.ERR)
        IF R.FT EQ '' THEN
            CALL EB.READ.HISTORY.REC(F.FT.HIS,Y.ID,R.FT,FT.HIS.ERR)
        END
        R.NEW(NM.ADVICE.NO) = R.FT<FT.LOCAL.REF,Y.ADVISE.REF.NO.POS>
    END
    ELSE
        CALL F.READ(FN.TT,Y.TT.ID,R.TT,F.TT,TT.ERR)
        IF R.TT EQ '' THEN
            CALL EB.READ.HISTORY.REC(F.TT.HIS,Y.ID,R.TT,TT.HIS.ERR)
        END
        R.NEW(NM.ADVICE.NO) = R.TT<TT.TE.LOCAL.REF,Y.TT.ADVISE.REF.NO.POS>
    END
    RETURN
END
