    SUBROUTINE FDR.MAT.EXCISE.DUTY.PROCESS(Y.AZ.ACCT)

    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT GLOBUS.BP I_ENQUIRY.COMMON
    $INSERT GLOBUS.BP I_F.AZ.ACCOUNT
    $INSERT GLOBUS.BP I_F.ACCOUNT
    $INSERT JBL.BP I_FDR.MAT.EXCISE.DUTY.PROCESS.COMMON

    GOSUB MAIN.PROCESS
    RETURN

*------------
MAIN.PROCESS:
*------------
    FN.AZ.ACCT = 'FBNK.AZ.ACCOUNT'
    F.AZ.ACCT = ''
    CALL OPF(FN.AZ.ACCT,F.AZ.ACCT)

    FN.ACCT = 'FBNK.ACCOUNT'
    F.ACCT = ''
    CALL OPF(FN.ACCT,F.ACCT)

    CALL F.READ(FN.AZ.ACCT,Y.AZ.ACCT,REC.AZ,F.AZ.ACCT,Y.AZ.ERR)
    Y.LIQ.ACCT =REC.AZ<AZ.REPAY.ACCOUNT>
    CALL F.READ(FN.ACCT,Y.LIQ.ACCT,REC.ACCT,F.ACCT,Y.ACCT.ERR)

    IF REC.ACCT<AC.WORKING.BALANCE> LE 0 THEN
        Y.OFS.STR=''

        Y.OFS.STR :="MATURITY.DATE::=" : REC.AZ<AZ.MATURITY.DATE>
        Y.OFS.STR :=",REPAY.ACCOUNT::=" : REC.AZ<AZ.REPAY.ACCOUNT>
        Y.OFS.STR :=",NOMINATED.ACCOUNT::=" : REC.AZ<AZ.NOMINATED.ACCOUNT>
        Y.OFS.STR :=",INTEREST.RATE::=" : REC.AZ<AZ.INTEREST.RATE>
        Y.OFS.STR :=",TAX.ON::=" : REC.AZ<AZ.TAX.ON>
        Y.OFS.STR :=",TAX.KEY::=" : REC.AZ<AZ.TAX.KEY>
        Y.OFS.STR :=",REDEMPTION.AMT::=" : -1*REC.ACCT<AC.WORKING.BALANCE>
        Y.OFS.STR :=",EARLY.RED.INT::=" : REC.AZ<AZ.INTEREST.RATE>
        Y.OFS.STR :=",CHG.FROM.RED.AMT::=" : 'N'
        Y.UPLOAD.COM = REC.AZ<AZ.CO.CODE>
        CALL LOAD.COMPANY(Y.UPLOAD.COM)

        Y.OFS.MSG.POST = "AZ.ACCOUNT,MIG/I/PROCESS,//":Y.UPLOAD.COM:",":Y.AZ.ACCT:",": Y.OFS.STR

        CALL OFS.GLOBUS.MANAGER(Y.SOURCE,Y.OFS.MSG.POST)
        Y.OFS.ERR.CODE = FIELD(FIELD(Y.OFS.MSG.POST,"/",3),',',1)
    END
    RETURN
END
