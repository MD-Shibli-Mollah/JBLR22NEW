    SUBROUTINE SS.FIELD.LIST(Y.RETURN)

    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT GLOBUS.BP I_F.STANDARD.SELECTION

    FN.SS = 'F.STANDARD.SELECTION'
    F.SS = ''
    R.SS = ''
    SS.APP = ID.NEW

    CALL OPF(FN.SS,F.SS)
    CALL F.READ(FN.SS,SS.APP,R.SS,F.SS,ERR.SS)
    IF R.SS THEN
        FOR I = 1 TO DCOUNT(R.SS<SSL.SYS.FIELD.NAME>,@VM)
            Y.RETURN<-1> = R.SS<SSL.SYS.FIELD.NAME,I>
        NEXT
    END
    RETURN
END
