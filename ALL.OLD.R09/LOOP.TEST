    PROGRAM LOOP.TEST

    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT GLOBUS.BP I_F.ACCOUNT

    FN.ACC = 'F.ACCOUNT'
    F.ACC = ''
    Y.AC.ID = 0100000510917
    R.ACC = ''
    I=0

    CALL OPF(FN.ACC,F.ACC)

    LOOP
        WHILE(I LT 1000)
        CALL F.READ(FN.ACC,Y.AC.ID,R.ACC,F.ACC,ERR.ACC)
        Y.CUS.ID = R.ACC<AC.CUSTOMER>
        Y.CATEG = R.ACC<AC.CATEGORY>
        CRT Y.CUS.ID:'*':Y.CATEG
        I++
    REPEAT
END
