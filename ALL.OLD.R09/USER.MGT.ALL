    SUBROUTINE USER.MGT.ALL
    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT BP I_F.USER.MGT


    FN.USER.MGT = 'F.EB.USER.MGT'
    F.USER.MGT = ''

    CALL OPF(FN.USER.MGT,F.USER.MGT)

    Y.REQUEST="EB.USER.MGT":PGM.VERSION
    IF Y.REQUEST EQ "EB.USER.MGT,AMEND" THEN
        R.NEW(EB.USE61.REQUEST.STATUS)="PENDING"
        R.NEW(EB.USE61.REQUEST.FOR)="AMEND"
    END
END
