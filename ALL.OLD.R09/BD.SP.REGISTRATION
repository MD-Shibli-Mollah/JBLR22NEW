    SUBROUTINE BD.SP.REGISTRATION
    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT GLOBUS.BP I_ENQUIRY.COMMON


    Y.REG.NO=O.DATA
    Y.REG1=FIELD(Y.REG.NO,".",1)
    Y.REG2=FIELD(Y.REG.NO,".",2)
    Y.REG.BB=Y.REG1:LEFT(FIELD(Y.REG.NO,".",2),1):RIGHT(FIELD(Y.REG.NO,".",2),4):"/20":SUBSTRINGS(Y.REG2,3,2)
    O.DATA=Y.REG.BB

    RETURN
END
