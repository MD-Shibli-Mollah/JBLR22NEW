!PROGRAM JBL.PORT.INV.FLD
    SUBROUTINE JBL.PORT.INV.FLD
    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT GLOBUS.BP I_ENQUIRY.COMMON
    $INSERT BP I_F.JBL.PORT.INVOICE.INFO
!DEBUG
    FN.INVOICE.INFO = 'F.JBL.PORT.INVOICE.INFO'
    F.INVOICE.INFO =''
    CALL OPF(FN.INVOICE.INFO,F.INVOICE.INFO)
    Y.DATE=RIGHT(LEFT(TODAY,4),2)
    IF R.NEW(PORT.INV.CHALLAN.NO) EQ ""  THEN
        R.NEW(PORT.INV.CHALLAN.NO)=Y.DATE:"/"
    END
    
    RETURN
