    SUBROUTINE JBL.ATM.MAINT.CALC.ID

    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT GLOBUS.BP I_Table
    $INCLUDE BP I_F.JBL.ATM.MAINT.CALC

    IF ID.NEW EQ 'SYSTEM' OR ID.NEW EQ 'ISSUE' OR ID.NEW EQ 'REISSUE' OR ID.NEW EQ 'CLOSE' OR ID.NEW EQ 'PINREISSUE' THEN
    END  ELSE
        E = "INVALID ID"
    END
    IF E THEN
        T.SEQU = "IFLD"
        CALL ERR
    END

    RETURN
