    SUBROUTINE USER.FROM.DATE
!PROGRAM USER.FROM.DATE
    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT BP I_F.USER.MGT
    $INSERT GLOBUS.BP I_F.TELLER.ID
    DEBUG
    FN.USER.MGT = 'F.EB.USER.MGT'
    F.USER.MGT = ''
    CALL OPF(FN.USER.MGT,F.USER.MGT)

    Y.FROM.DATE = R.NEW(EB.USE61.STOP.START.DATE)
    Y.TO.DATE = R.NEW(EB.USE61.STOP.END.DATE)
    CRT Y.FROM.DATE

END
