    SUBROUTINE JBL.SMS.TEMP.ID
    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT JBL.BP I_F.SMS.TEMPLATE.DEFINE

    FN.FILE.CON = 'F.FILE.CONTROL'
    F.FILE.CON = ''
    R.FILE.CON = ''
    ERR.FILE.CON = ''

    CALL OPF(FN.FILE.CON,F.FILE.CON)
    CALL F.READ(FN.FILE.CON,ID.NEW,R.FILE.CON,F.FILE.CON,ERR.FILE.CON)
    IF NOT(R.FILE.CON) THEN
        E = 'NO RECORD FOUND IN FILE.CONTROL'
    END
    RETURN
END
