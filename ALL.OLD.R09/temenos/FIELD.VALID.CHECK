    SUBROUTINE FIELD.VALID.CHECK
!PROGRAM FIELD.VALID.CHECK
    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT BP I_F.USER.MGT

    Y.TODAY = TODAY

    FN.USER.MGT = 'F.EB.USER.MGT'
    F.USER.MGT = ''

    CALL OPF(FN.USER.MGT,F.USER.MGT)

    Y.CLOSE.APPLY.DATE = COMI
    IF Y.TODAY GT Y.CLOSE.APPLY.DATE THEN
        ETEXT = "DATE MUST BE TODAY OR FUTURE DATE "
        CALL STORE.END.ERROR
    END
END
