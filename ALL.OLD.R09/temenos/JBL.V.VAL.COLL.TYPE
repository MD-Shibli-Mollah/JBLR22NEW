    SUBROUTINE JBL.V.VAL.COLL.TYPE

    $INCLUDE GLOBUS.BP I_COMMON
    $INCLUDE GLOBUS.BP I_EQUATE
    $INCLUDE GLOBUS.BP I_F.COLLATERAL

    COLL.TYPE = ''

    Y.APPLICATION       = "COLLATERAL"

    FIELD.NAME          = "ACCT.NO":VM:"BORROWER":VM:"CUS.ADDRESS":VM:"PHONE.NUMBER":VM:"MOBILE.NUMBER":VM:"LIMIT.REFERENCE"
    FIELD.NAME:= VM:"LIMIT":VM:"L.START.DATE":VM:"L.EXPIRY.DATE":VM:"DP.S.DATE":VM:"DP.EX.DATE":VM:"NAME.OF.STOCK"
    FIELD.NAME:= VM:"DATE.OF.STOCK":VM:"QUANTITY":VM:"VALUE.PER.UNIT":VM:"TOTAL.PER.STOCK":VM:"MARGIN":VM:"AMOUNT.MARGIN":VM:"DRAWING"
    FIELD.NAME:= VM:"TOT.STOCK":VM:"TOT.MARGIN":VM:"TOT.POWER":VM:"ADDR.GODOWN":VM:"REMARKS"

    FIELD.POS           = ""
    CALL MULTI.GET.LOC.REF(Y.APPLICATION,FIELD.NAME,FIELD.POS)

    ACCT.NO.POS  = FIELD.POS<1,1>
    BORROWER.POS  = FIELD.POS<1,2>
    CUS.ADDRESS.POS  = FIELD.POS<1,3>
    PHONE.NUMBER.POS     = FIELD.POS<1,4>
    MOBILE.NUMBER.POS  = FIELD.POS<1,5>
    LIMIT.REFERENCE.POS = FIELD.POS<1,6>
    LIMIT.POS  = FIELD.POS<1,7>
    L.START.DATE.POS  = FIELD.POS<1,8>
    L.EXPIRY.DATE.POS  = FIELD.POS<1,9>
    DP.S.DATE.POS  = FIELD.POS<1,10>
    DP.EX.DATE.POS  = FIELD.POS<1,11>
    NAME.OF.STOCK.POS  = FIELD.POS<1,12>
    DATE.OF.STOCK.POS  = FIELD.POS<1,13>
    QUANTITY.POS  = FIELD.POS<1,14>
    VALUE.PER.UNIT.POS  = FIELD.POS<1,15>
    TOTAL.PER.STOCK.POS  = FIELD.POS<1,16>
    MARGIN.POS  = FIELD.POS<1,17>
    AMOUNT.MARGIN.POS  = FIELD.POS<1,18>
    DRAWING.POS  = FIELD.POS<1,19>
    TOT.STOCK.POS  = FIELD.POS<1,20>
    TOT.MARGIN.POS  = FIELD.POS<1,21>
    TOT.POWER.POS  = FIELD.POS<1,22>
    ADDR.GODOWN.POS  = FIELD.POS<1,23>
    REMARKS.POS  = FIELD.POS<1,24>

    COLL.TYPE = COMI

    IF COLL.TYPE NE 210 THEN


        T.LOCREF<ACCT.NO.POS,7>  = "NOINPUT"
        T.LOCREF<BORROWER.POS,7>  = "NOINPUT"
        T.LOCREF<CUS.ADDRESS.POS,7>  = "NOINPUT"
        T.LOCREF<PHONE.NUMBER.POS,7>  = "NOINPUT"
        T.LOCREF<MOBILE.NUMBER.POS,7>  = "NOINPUT"
        T.LOCREF<LIMIT.REFERENCE.POS,7>  = "NOINPUT"
        T.LOCREF<LIMIT.POS,7>  = "NOINPUT"
        T.LOCREF<L.START.DATE.POS,7>  = "NOINPUT"
        T.LOCREF<L.EXPIRY.DATE.POS,7>  = "NOINPUT"
        T.LOCREF<DP.S.DATE.POS,7>  = "NOINPUT"
        T.LOCREF<DP.EX.DATE.POS,7>  = "NOINPUT"
        T.LOCREF<NAME.OF.STOCK.POS,7>  = "NOINPUT"
        T.LOCREF<DATE.OF.STOCK.POS,7>  = "NOINPUT"
        T.LOCREF<QUANTITY.POS,7>  = "NOINPUT"
        T.LOCREF<VALUE.PER.UNIT.POS,7>  = "NOINPUT"
        T.LOCREF<TOTAL.PER.STOCK.POS,7>  = "NOINPUT"
        T.LOCREF<MARGIN.POS ,7>  = "NOINPUT"
        T.LOCREF<AMOUNT.MARGIN.POS,7>  = "NOINPUT"
        T.LOCREF<DRAWING.POS,7>  = "NOINPUT"
        T.LOCREF<TOT.STOCK.POS,7>  = "NOINPUT"
        T.LOCREF<TOT.MARGIN.POS,7>  = "NOINPUT"
        T.LOCREF<TOT.POWER.POS,7>  = "NOINPUT"
        T.LOCREF<ADDR.GODOWN.POS,7>  = "NOINPUT"
        T.LOCREF<REMARKS.POS,7>  = "NOINPUT"

    END
    RETURN
END
