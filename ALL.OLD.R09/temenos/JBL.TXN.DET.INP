    SUBROUTINE JBL.TXN.DET.INP

    $INSERT GLOBUS.BP I_EQUATE
    $INSERT GLOBUS.BP I_COMMON
    $INSERT JBL.BP I_F.ST.JBL.TXN.DETAIL

    FN.TXN = 'F.ST.JBL.TXN.DETAIL'
    F.TXN = ''
    CALL OPF(FN.TXN,F.TXN)

    IF V$FUNCTION EQ 'I' THEN
        IF R.NEW(ST.JBL11.TXN.AMT) NE (R.NEW(ST.JBL11.PRIN.AMT) + R.NEW(ST.JBL11.INT.AMT)) THEN
            ETEXT = 'TOTAL TXN AMOUNT NOT EQ PRINCIPAL+INTREST AMOUNT'
            CALL STORE.END.ERROR
        END
    END

    RETURN
END
