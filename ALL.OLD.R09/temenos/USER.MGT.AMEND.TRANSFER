    SUBROUTINE USER.MGT.AMEND.TRANSFER
!PROGRAM USER.MGT.AMEND.TRANSFER
    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT BP I_F.USER.MGT

    FN.USER.MGT = 'F.EB.USER.MGT'
    F.USER.MGT = ''

    CALL OPF(FN.USER.MGT,F.USER.MGT)

    Y.REQUEST.REASON = COMI
    IF Y.REQUEST.REASON EQ 'TRANSFER' THEN
        ETEXT = "Please do not Select Transfer"
        CALL STORE.END.ERROR
    END
END
