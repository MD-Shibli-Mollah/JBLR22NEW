    SUBROUTINE JBL.COM.CHECK
!PROGRAM JBL.COM.CHECK
    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT BP I_F.USER.MGT

    FN.USER.MGT = 'F.EB.USER.MGT'
    F.USER.MGT = ''

    CALL OPF(FN.USER.MGT,F.USER.MGT)

    Y.COMPANY = ID.COMPANY
    Y.COM.VALID = COMI

    IF Y.COMPANY EQ Y.COM.VALID THEN
        ETEXT = "USER TRANSFER MUST BE POSSIBLE FOR ANOTHER BRANCH.PLEASE INPUT DESIRED BRANCH CODE."
        CALL STORE.END.ERROR
    END
END
