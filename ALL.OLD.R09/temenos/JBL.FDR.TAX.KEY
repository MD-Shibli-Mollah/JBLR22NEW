    SUBROUTINE JBL.FDR.TAX.KEY

    $INSERT GLOBUS.BP I_COMMON
    $INSERT GLOBUS.BP I_EQUATE
    $INSERT GLOBUS.BP I_ENQUIRY.COMMON
    $INSERT GLOBUS.BP I_F.AZ.ACCOUNT

    GOSUB INIT
    GOSUB OPENFILES
    GOSUB PROCESS

*-------
INIT:
*-------
    FN.AZ.ACCT = 'FBNK.AZ.ACCOUNT'
    F.AZ.ACCT = ''
    RETURN

*---------
OPENFILES:
*---------
    CALL OPF(FN.AZ.ACCT,F.AZ.ACCT)
    RETURN

*---------
PROCESS:
*---------
    IF COMI NE 'I' THEN
        R.NEW(AZ.TAX.KEY) = ' '
    END ELSE
        R.NEW(AZ.TAX.KEY) = '11'
    END
    RETURN
