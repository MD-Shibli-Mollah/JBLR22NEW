    SUBROUTINE JBL.S.FDR.ROLL.DET
    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT I_F.AZ.ACCOUNT

    BEGIN CASE

    CASE R.NEW(AZ.ALL.IN.ONE.PRODUCT) EQ 'FIXEDDEP1M'
        R.NEW(AZ.ROLLOVER.INT.RATE)='P03'
        R.NEW(AZ.ROLLOVER.TERM)='1M'
        R.NEW(AZ.SCHEDULES)='N'
        R.NEW(AZ.CALCULATION.BASE)='PRINCIPAL'

    CASE R.NEW(AZ.ALL.IN.ONE.PRODUCT) EQ 'FD2-MONTH'
        R.NEW(AZ.ROLLOVER.INT.RATE)='P04'
        R.NEW(AZ.ROLLOVER.TERM)='2M'
        R.NEW(AZ.SCHEDULES)='N'
        R.NEW(AZ.CALCULATION.BASE)='PRINCIPAL'

    CASE R.NEW(AZ.ALL.IN.ONE.PRODUCT) EQ 'FIXEDDEP3-6M'
        R.NEW(AZ.ROLLOVER.INT.RATE)='P02'
        R.NEW(AZ.ROLLOVER.TERM)='3M'
        R.NEW(AZ.SCHEDULES)='N'
        R.NEW(AZ.CALCULATION.BASE)='PRINCIPAL'

    CASE R.NEW(AZ.ALL.IN.ONE.PRODUCT) EQ 'FIXEDDEP6-12M'
        R.NEW(AZ.ROLLOVER.INT.RATE)='P02'
        R.NEW(AZ.ROLLOVER.TERM)='3M'
        R.NEW(AZ.ROLL.AIO.PRODUCT)='FIXEDDEP3-6M'
        R.NEW(AZ.SCHEDULES)='N'
        R.NEW(AZ.CALCULATION.BASE)='PRINCIPAL'

    CASE R.NEW(AZ.ALL.IN.ONE.PRODUCT) EQ 'FIXEDDEP1-2Y'
        R.NEW(AZ.SCHEDULES)='Y'
        R.NEW(AZ.TYPE.OF.SCHDLE)='I'
        R.NEW(AZ.CALCULATION.BASE)='SCHEDULED BALANCE'

    CASE R.NEW(AZ.ALL.IN.ONE.PRODUCT) EQ 'FIXEDDEP2-3Y'
        R.NEW(AZ.SCHEDULES)='Y'
        R.NEW(AZ.TYPE.OF.SCHDLE)='I'
        R.NEW(AZ.CALCULATION.BASE)='SCHEDULED BALANCE'

    CASE 1

    END CASE
    RETURN
END
